<config>
	<process key='MAIN'>
		<transitions>
		  <transition to='DOC' />
		  <transition from='DOC' event='doc.end' />
		  <transition from='DOC' to='DOC' />
		</transitions>
		<states>
			<state key='DOC'>
			<transitions>
                <transition to='#INIT' />
                <transition from='#INIT' to='#B' />
				<transition from='#B' to='#B' />
				<transition from='*' event='doc.end' />
			 </transitions>
			 <states>
                <state key='#IB'>
                    <transitions>
                        <!-- #block -->
                        <transition from='*' event='block.verbatim' to='BLOCK_VERBATIM' />
                        <transition from='*' event='block.macro' to='BLOCK_MACRO' />
                        <transition from='*' event='block.extension' to='BLOCK_EXTENSION' />
                        <transition from='*' event='doc.begin' to='DOC' />
                        <transition from='DOC' event='doc.end' to='{initial}' />
                        <transition to='TEXT' />
                        <!-- /#block -->
                    </transitions>
                    <states>
                    </states>
                </state>s
				<state key='#B'>
					<transitions>
						<transition event='block.property' to='BLOCK_PROPERTY' />
						<transition event='block.info' to='BLOCK_INFO' />
						<transition event='block.quot' to='BLOCK_QUOT' />
                        <transition event='list' to='LIST' />
                        <transition event='dl' to='DL' />
						<transition event='table' to='TABLE' />
						<transition event='header' to='HEADER' />
						<transition event='hr' to='HR' />
						<transition event='empty' to='EMPTY_PARAGRAPH' />
						<!-- #block -->
						<transition event='block.verbatim' to='BLOCK_VERBATIM' />
						<transition event='block.macro' to='BLOCK_MACRO' />
						<transition event='block.extension' to='BLOCK_EXTENSION' />
                        <transition event='doc' to='DOC' />
                        <transition from='DOC' event='doc.end' to='{initial}' />
						<!-- /#block -->
						<transition to='P' />
					</transitions>
					<states>
                        <state key='P'>
                            <transitions>
                                <transition to='TEXT' />
                            </transitions>
                        </state>
                        <state key='HEADER'>
                            <transitions>
                                <transition to='TEXT' />
                            </transitions>
                        </state>
                        <state key='TABLE'>
                            <transitions>
                                <transition to='TR' />
                                <transition from='TR' event='table.tr' to='TR' />
                            </transitions>
                            <states>
                                <state key='TR'>
                                    <transitions>
                                        <transition event='table.tr.th' to='TH' />
                                        <transition to='TD' />
                                        <transition from='*' event='table.tr.td' to='TD' />
                                        <transition from='*' event='table.tr.th' to='TH' />
                                    </transitions>
                                    <states>
                                        <state key='TD'>
                                            <transitions>
                                                <transition to='#IB' />
                                            </transitions>
                                        </state>
                                        <state key='TH'>
                                            <transitions>
                                                <transition to='#IB' />
                                            </transitions>
                                        </state>
                                    </states>
                                </state>
                            </states>
                        </state>
                        <state key='LIST'>
                            <transitions>
                                <transition to='LI' />
                                <transition from='LI' event='list.li' to='LI' />
                            </transitions>
                            <states>
                                <state key='LI'>
                                    <transitions>
                                        <transition event='list' to='#IB' />
                                        <transition to='#IB' />
                                        <transition from='*' event='list' to='LIST' />
                                        <transition from='*' event='list.li' />
                                        <transition from='*' event='list.end' />
                                        <transition from='*' event='dl' to='DL' />
                                        <transition from='*' event='dl.end' />
                                    </transitions>
                                    <states>
                                    </states>
                                </state>
                            </states>
                        </state>
                        <state key='DL'>
                            <transitions>
                                <transition from='*' event='dl.dt' to='DT' />
                                <transition to='DD' />
                            </transitions>
                            <states>
                                <state key='DD'>
                                    <transitions>
                                        <transition event='dl' to='#IB' />
                                        <transition to='#IB' />
                                        <transition from='*' event='list' to='LIST' />
                                        <transition from='*' event='list.end' />
                                        <transition from='*' event='dl' to='DL' />
                                        <transition from='*' event='dl.end' />
                                        <transition from='*' event='dl.dd' />
                                        <transition from='*' event='dl.dt' />
                                    </transitions>
                                    <states>
                                    </states>
                                </state>
                                <state key='DT'>
                                    <transitions>
                                        <transition to='TEXT' />
                                    </transitions>
                                </state>
                            </states>
                        </state>
					</states>
				</state>
			</states>
			</state>
		</states>
	</process>
</config>
